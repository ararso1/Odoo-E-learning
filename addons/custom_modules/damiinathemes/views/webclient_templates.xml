<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="damiinathemes.layoutpage" inherit_id="web.layout" name="Habesha themes Web layout">
        <xpath expr="//title" position="replace">
            <title t-if ='title' t-esc="title.replace('Odoo','Damiina').replace('odoo','Damiina') or 'Damiina'"/>
        </xpath>
        <xpath expr="//link[@rel='shortcut icon']" position="replace">
                <link type="image/x-icon" rel="shortcut icon" t-att-href="'/damiinathemes/static/description/icon.png'"/>
        </xpath>

    </template>
</odoo>
