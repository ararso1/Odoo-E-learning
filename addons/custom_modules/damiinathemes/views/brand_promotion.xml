<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="brand_promotion_message_inherit" inherit_id="web.brand_promotion_message">
        <xpath expr="//t[@t-set='final_message']" position="replace">
            <t t-set="final_message">Powered by Daminaa</t>
        </xpath>
        <xpath expr="//t[@t-set='final_message']/following-sibling::t" position="replace">
            <t t-out="final_message"></t>
        </xpath>
    </template>
</odoo>